<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="default">
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <uri>model://sun</uri>
    </include>
    <physics type="ode">

      <!-- run at 400Hz -->
      <max_step_size>0.002</max_step_size>         
      <real_time_update_rate>500</real_time_update_rate>
    </physics>

    <!-- reference to your plugin -->
    <plugin name="world_plugin" filename="libworld_plugin.so"/>

  </world>
 
</sdf>