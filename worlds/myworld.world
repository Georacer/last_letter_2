<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="default">
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <uri>model://sun</uri>
    </include>
    <physics type="ode">

      <!-- run at 400Hz -->
      <max_step_size>0.0025</max_step_size>         
      <real_time_update_rate>400</real_time_update_rate>
    </physics>

    <!-- reference to your plugin -->
    <plugin name="world_plugin" filename="libworld_plugin.so"/>

  </world>
 
   <rendering:gui>
    <type>fltk</type>
    <size>480 320</size>
    <pos>0 0</pos>
    <frames>
      <row height="100%">
        <camera width="100%">
          <xyz>0.3 0 3</xyz>
          <rpy>0 90 90</rpy>
        </camera>
      </row>
    </frames>
  </rendering:gui>
</sdf>