<robot name="plane" xmlns:xacro="http://www.ros.org/wiki/xacro">
	<link name="body">

		<visual>
		
			<geometry>
				<box size="2 1 0.5" />
			</geometry>
		</visual>

		<collision>
			<geometry>
				<box size="2 1 0.5" />
			</geometry>
		</collision>

		<inertial>
			<mass value="2" />
			<inertia ixx="1.04166666666666" ixy="0" ixz="0" iyz="0"
				iyy="3.5416666666666" izz="4.166666666666666" />
		</inertial>

	</link>
    ----------------------------------------------------------------------------  wheels

	---------------------------------------------------------------------------front wheels
     
	<link name="wheel_base_front">
		<visual>
			<geometry>
				<box size="0.05 2 0.05" />
			</geometry>
		</visual>

		<collision>
			<geometry>
				<box size="0.05 2 0.05" />
			</geometry>
		</collision>

		<inertial>
			<mass value="0.1" />
			<inertia ixx="0.033354166666" ixy="0" ixz="0" iyz="0"
				iyy=" 0.00004166666" izz="0.033354166666" />
		</inertial>

	</link>

	<joint name=" body_to_wheel_base_front" type="fixed">
		<origin xyz="1 0 -0.25" rpy="0 0 0" />
		<parent link="body" />
		<child link="wheel_base_front" />
	</joint>

	<link name="wheel_base_rear">
		<visual>
			<geometry>
				<box size="0.05 2 0.05" />
			</geometry>
		</visual>

		<collision>
			<geometry>
				<box size="0.05 2 0.05" />
			</geometry>
		</collision>

		<inertial>
			<mass value="0.1" />
			<inertia ixx="0.033354166666" ixy="0" ixz="0" iyz="0"
				iyy=" 0.00004166666" izz="0.033354166666" />
		</inertial>

	</link>

	<joint name=" body_to_wheel_rear" type="fixed">
		<origin xyz="-1 0 -0.25" rpy="0 0 0" />
		<parent link="body" />
		<child link="wheel_base_rear" />
	</joint>

	<link name="left_wheel_front">
		<visual>
			<origin xyz="0 0 0.05" />
			<geometry>
				<cylinder radius="0.3" length="0.1" />
			</geometry>
		</visual>

		<collision>
			<origin xyz="0 0 0.05" />
			<geometry>
				<cylinder radius="0.3" length="0.1" />
			</geometry>
		</collision>

		<inertial>
			<origin xyz="0 0 0.05" />
			<mass value="0.1" />
			<inertia ixx="0.0023333333333333" ixy="0" ixz="0" iyz="0"
				iyy=" 0.0023333333333333" izz="0.00075" />
		</inertial>
	</link>

	<joint name=" wheel_base_to_left_wheel_front" type="continuous">
		<origin xyz="0 -1 0" rpy="1.57 0 0" />
		<parent link="wheel_base_front" />
		<child link="left_wheel_front" />
		<axis xyz=" 0 0 1" />
	</joint>


	<link name="right_wheel_front">
		<visual>
			<origin xyz="0 0 -0.05" />
			<geometry>
				<cylinder radius="0.3" length="0.1" />
			</geometry>
		</visual>

		<collision>
			<origin xyz="0 0 -0.05" />
			<geometry>
				<cylinder radius="0.3" length="0.1" />
			</geometry>
		</collision>

		<inertial>
			<origin xyz="0 0 -0.05" />
			<mass value="0.1" />
			<inertia ixx="0.0023333333333333" ixy="0" ixz="0" iyz="0"
				iyy=" 0.0023333333333333" izz="0.00075" />
		</inertial>
	</link>

	<joint name=" wheel_base_to_right_wheel_front" type="continuous">
		<origin xyz="0 1 0" rpy="1.57 0 0" />
		<parent link="wheel_base_front" />
		<child link="right_wheel_front" />
		<axis xyz=" 0 0 1" />
	</joint>

	--
	<link name="left_wheel_rear">
		<visual>
			<origin xyz="0 0 0.05" />
			<geometry>
				<cylinder radius="0.3" length="0.1" />
			</geometry>
		</visual>

		<collision>
