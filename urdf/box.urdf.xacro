		<inertial>
			<origin xyz="0 0 0.05" />
			<mass value="0.1" />
			<inertia ixx="0.0023333333333333" ixy="0" ixz="0" iyz="0"
				iyy=" 0.0023333333333333" izz="0.00075" />
		</inertial>
	</link>

	<joint name=" wheel_base_to_left_wheel_rear" type="continuous">
		<origin xyz="0 -1 0" rpy="1.57 0 0" />
		<parent link="wheel_base_rear" />
		<child link="left_wheel_rear" />
		<axis xyz=" 0 0 1" />
	</joint>


	<link name="right_wheel_rear">
		<visual>
			<origin xyz="0 0 -0.05" />
			<geometry>
				<cylinder radius="0.3" length="0.1" />
			</geometry>
		</visual>

		<collision>
			<origin xyz="0 0 -0.05" />
			<geometry>
				<cylinder radius="0.3" length="0.1" />
			</geometry>
		</collision>

		<inertial>
			<origin xyz="0 0 -0.05" />
			<mass value="0.1" />
			<inertia ixx="0.0023333333333333" ixy="0" ixz="0" iyz="0"
				iyy=" 0.0023333333333333" izz="0.00075" />
		</inertial>
	</link>

	<joint name=" wheel_base_to_right_wheel_rear" type="continuous">
		<origin xyz="0 1 0" rpy="1.57 0 0" />
		<parent link="wheel_base_rear" />
		<child link="right_wheel_rear" />
		<axis xyz=" 0 0 1" />
	</joint>

	<gazebo>
    <plugin name="motor_plugin" filename="libmotor_plugin.so"/>
  </gazebo>

</robot>